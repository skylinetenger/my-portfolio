{
  "name": "my-portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://skylinetenger.github.io/my-portfolio/",
  "dependencies": {
    "font-awesome": "^4.7.0",
    "gh-pages": "^1.2.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "sass": "node-sass -w src/scss/ -o src/css/ --recursive",
    "build-sass": "node-sass src/scss/ -o src/css/",
    "start": "react-scripts start",
    "dev": "npm-run-all -p sass start",
    "build": "react-scripts build",
    "dev-build": "npm-run-all build-sass build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
